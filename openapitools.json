{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.1",
    "useDocker": true,
    "generators": {
      "angular-client": {
        "generatorName": "typescript-angular",
        "output": "./generated/angular_client",
        "inputSpec": "./api_spec/library.openapi.yaml"
      },
      "java-client": {
        "generatorName": "java",
        "output": "./generated/java_client",
        "inputSpec": "./api_spec/library.openapi.yaml"
      },
      "perl-client": {
        "generatorName": "perl",
        "output": "./generated/perl-client",
        "inputSpec": "./api_spec/library.openapi.yaml"
      },
      "kotlin-server": {
        "generatorName": "kotlin-spring",
        "output": "./generated/kotlin-server",
        "inputSpec": "./api_spec/library.openapi.yaml"
      }
    }
  }
}
